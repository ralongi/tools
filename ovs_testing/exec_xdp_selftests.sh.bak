#!/bin/bash

set -x
if [[ -z $COMPOSE ]]; then COMPOSE=$1; fi
dut=${dut:-"wsfd-advnetlab33.anl.eng.rdu2.dc.redhat.com"}

# ebpf_xdp/xdp_selftests

pushd ~/temp

lstest ~/git/kernel/networking/ebpf_xdp/xdp_selftests | runtest $COMPOSE --arch=x86_64 --topo=singleHost --systype=machine --machine=$dut $(echo "$zstream_repo_list") $(echo "$cmds") $(echo "$brew_build_cmd") --wb "(DUT: $dut), XDP Tools selftests, $COMPOSE, networking/ebpf_xdp/xdp_selftests $special_info" --append-task="/kernel/networking/openvswitch/crash_check {dbg_flag=set -x}"

popd
